<% @drive_files.each do |file| %>
	<div class="well messagebody">
	 <%# case for new image %>
		<h1> <%= file.name %> </h1>
		<p> <%= file.type %> </p>
		<p> Owner: <%= file.owner %> </p>
	</div>
<% end %>
<div class="text-center"><%= link_to "Upload new file", 'files/new', class: "btn btn-warning btn-lg" %></div>